package concurrencia;

/**
 *
 * @author T-107
 */
public class TercerThread extends Thread {
     public void run()
    {
        while(true){
        try 
        {
         Thread.sleep(3000);
        System.out.println("Yo soy el  tercer Tread 7v7");
}
        //nunca se lanza
        catch(InterruptedException e){}
    
    }
        }
}
